               <Font size="20.0" />
            </font>
         </Label>
         <Slider fx:id="eventSlider" blockIncrement="1.0" layoutX="333.0" layoutY="400.0" majorTickUnit="4.0" max="20.0" min="1.0" prefHeight="16.0" prefWidth="202.0" showTickLabels="true" showTickMarks="true" value="1.0" />
