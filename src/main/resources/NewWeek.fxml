            <font>
               <Font size="40.0" />
            </font>
         </Label>
         <Label layoutX="218.0" layoutY="98.0" text="Name of Week">
            <font>
               <Font size="25.0" />
            </font>
         </Label>
         <TextField fx:id="newName" layoutX="215.0" layoutY="153.0" />
         <Label layoutX="208.0" layoutY="214.0" text="Select New Path">
            <font>
               <Font size="25.0" />
            </font>
         </Label>
         <Button fx:id="newPath" styleClass="btn" layoutX="546.0" layoutY="264.0" mnemonicParsing="false" prefHeight="0.0" prefWidth="0.0" text="...">
