<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@style.css">
    <AnchorPane styleClass="page" prefHeight="571.0" prefWidth="590.0" stylesheets="@style.css">
      <children>
